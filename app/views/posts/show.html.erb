<p class="well">
  <b>Title:</b>
  <%= @post.title %>
</p>

<p class="well">
  <b>Body:</b>
  <%= @post.body %>
</p>

<p class="well">
  <b>Published at:</b>
  <%= @post.published_at %>
</p>


<%= link_to 'Edit', edit_post_path(@post), class: 'btn btn-warning'  %> |
<%= link_to 'Back', posts_path, class: 'btn btn-inverse'%>
